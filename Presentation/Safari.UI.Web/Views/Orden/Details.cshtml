@model Safari.Entities.Orden

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<link href="~/Areas/Admin/Content/css/sb-admin-2.css" rel="stylesheet" />

<div class="col-auto" >
    <h4>Detalle de la respuesta</h4>
    <hr />
    <dl c >
        <dt>
            Pregunta
        </dt>

        <dd>
            @Html.DisplayFor(model => model.pregunta.LaPregunta)
        </dd>

        <dt>
            Respuesta
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LaRespuesta)
        </dd>

        <dt>
            Orden
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NumeroOrden)
        </dd>

    </dl>
    <div>
        <p>
            @Html.ActionLink("Editar", "Edit", new { id = Model.Id })
            <input type="button" class="AzulButton " style="float:right" value="Volver" onclick="location.href='@Url.Action("Index", "Orden")'" />
        </p>
       
    </div>
    </div>
   
