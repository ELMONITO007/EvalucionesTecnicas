@model IEnumerable<Safari.Entities.Nivel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<link href="~/Areas/Admin/Content/css/sb-admin-2.css" rel="stylesheet" />

<div>
    <table class="table">
        <tr>
            <th>
              <h4>Nivel de Pregunta</h4>
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ElNivel)
                </td>
                <td>
                    @Html.ActionLink("Editar", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Detalles", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Eliminar", "Delete", new { id = item.Id })
                </td>
            </tr>
        }





    </table>

    <div >
       <p>           <input type="button" class="AzulButton "  style="float:right" value="Crear" onclick="location.href='@Url.Action("Create", "Nivel")'" /></p>
        

    </div>
</div>
