@model Safari.Entities.Pregunta

@{
    ViewBag.Title = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<link href="~/Areas/Admin/Content/css/sb-admin-2.css" rel="stylesheet" />
<div style="max-width:700px">
    <h3 class="col-md-10">¿Esta seguro que quiere eliminar la Pregunta @Html.DisplayFor(model => model.Id)? </h3>
    <hr />
    

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

<div >
    <input type="button" class="AzulButton " style="float:right" value="Volver" onclick="location.href='@Url.Action("Index", "Pregunta")'" />
    <input type="submit" value="Eliminar" class="AzulButton" />
</div>
    }
</div>
