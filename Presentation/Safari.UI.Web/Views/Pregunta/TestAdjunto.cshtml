@model Safari.Entities.Pregunta

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<link href="~/Areas/Admin/Content/css/sb-admin-2.css" rel="stylesheet" />





<div class="form-horizontal">

    <h2>Crear una Pregunta</h2>

    <hr />

   

 
    
    <form method="post" enctype="multipart/form-data">
        <div class="col-md-10">
          
        </div>
        <div class="col-md-10">
            <h5>Pregunta</h5>
            @Html.EditorFor(model => model.LaPregunta, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.LaPregunta, "", new { @class = "text-danger" })
            <br />

            <h5>Ubicacion Imagen</h5>
            <input type="file" name="file" accept="image/*" />
            <br />
            <br />

            <button type="submit" , class="AzulButton">Crear</button>

        </div>
    </form>





</div>



